<?xml version="1.0"?>
<extension key="uk.co.compucorp.emailfiling" type="module">
  <file>emailfiling</file>
  <name>Email Filing</name>
  <description>Allows to attach a copy of civicrm emails (inbound/outbound) as .eml file on activity.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Ivan Klochko @ Compucorp</author>
    <email>info@compucorp.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/compucorp/uk.co.compucorp.emailfiling</url>
    <url desc="Documentation">https://github.com/compucorp/uk.co.compucorp.emailfiling</url>
    <url desc="Support">https://github.com/compucorp/uk.co.compucorp.emailfiling/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-10-20</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.24</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr4 prefix="Civi\Emailfiling\" path="src"/>
  </classloader>
  <civix>
    <namespace>CRM/Emailfiling</namespace>
  </civix>
</extension>
